<tr>
  <td><%= loan_application_counter + 1 %></td>
  <td><%= loan_application.business_name %></td>
  <td><%= loan_application.established_year %></td>
  <td><%= loan_application.account_provider %></td>
  <td><%= loan_application.amount %></td>
  <td>
    <% if loan_application.status.blank? %>
      <%= link_to "Review", loan_application_path(loan_application) %>
    <% else %>  
      <%= loan_application.status %>
    <% end %>
  </td>
</tr>